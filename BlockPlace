local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, IntroEnabled = false, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "Logged in!",
	Content = "You are logged in as"..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Player1 = "string"
_G.Player2 = "string"
_G.Coordinates = "string"

-- Script

function MakeScriptHub()
    local ScriptHub = OrionLib:MakeWindow({Name = "Block Place | DarkStarsGaming#6722", HidePremium = false, SaveConfig = true, IntroEnabled = true, IntroText = "Dark's Script", ConfigFolder = "OrionTest"})

    -- Admin Commands
    local AdminCommandsTab = ScriptHub:MakeTab({
        Name = "Self Commands",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    AdminCommandsTab:AddButton({
        Name = "Give Actions",
        Callback = function()
            local args = {
                [1] = ">getactions",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
          end    
    })

    AdminCommandsTab:AddButton({
        Name = "Hide Yourself",
        Callback = function()
            local args = {
                [1] = ">hide DarkStarzGaming",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
          end    
    })

    AdminCommandsTab:AddButton({
        Name = "Show Command List",
        Callback = function()
            local args = {
                [1] = ">cmds",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
          end    
    })

    AdminCommandsTab:AddTextbox({
        Name = "Crash",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">crash "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    AdminCommandsTab:AddTextbox({
        Name = "Banhistory",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">banhistory "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    AdminCommandsTab:AddTextbox({
        Name = "Ban",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">ban "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    AdminCommandsTab:AddTextbox({
        Name = "Unban",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">unban "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    AdminCommandsTab:AddTextbox({
        Name = "Spectate",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">spectate "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    -- Trolling

    local Troll = ScriptHub:MakeTab({
        Name = "Trolling",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    Troll:AddTextbox({
        Name = "Messgui",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">messgui "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    Troll:AddTextbox({
        Name = "Kill",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">kill "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    Troll:AddTextbox({
        Name = "Kick",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">kick "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })
    
    -- Teleporting

    local Teleport = ScriptHub:MakeTab({
        Name = "Teleporting",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    Teleport:AddLabel("Teleport player to another player")

    Teleport:AddTextbox({
        Name = "Player1",
        Default = "",
        TextDisappear = false,
        Callback = function(Value)
            _G.Player1 = Value
        end
    })

    Teleport:AddTextbox({
        Name = "Player2",
        Default = "",
        TextDisappear = false,
        Callback = function(Value)
            _G.Player2 = Value
        end
    })

    Teleport:AddButton({
        Name = "Teleport Player",
        Callback = function()
            local args = {
                [1] = ">teleport ".._G.Player1.." ".._G.Player2.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
          end    
    })

    Teleport:AddLabel("Teleport player to coordinates")

    Teleport:AddTextbox({
        Name = "Player",
        Default = "",
        TextDisappear = false,
        Callback = function(Value)
            _G.Player1 = Value
        end
    })

    Teleport:AddTextbox({
        Name = "Coordinates",
        Default = "",
        TextDisappear = false,
        Callback = function(Value)
            _G.Coordinates = Value
        end
    })

    Teleport:AddButton({
        Name = "Teleport Player",
        Callback = function()
            local args = {
                [1] = ">tp ".._G.Player1.." ".._G.Coordinates.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
          end    
    })

    -- Player Configuration
    local Player = ScriptHub:MakeTab({
        Name = "Player",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    Player:AddTextbox({
        Name = "Walkspeed",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">walkspeed "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    Player:AddTextbox({
        Name = "Jumppower",
        Default = "",
        TextDisappear = true,
        Callback = function(txt)
            local args = {
                [1] = ">jumppower "..txt.."",
                [2] = "All"
            }
            
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))    
        end
    })

    -- Settings

    local Settings = ScriptHub:MakeTab({
        Name = "Settings",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    Settings:AddButton({
        Name = "Destroy UI!",
        Callback = function()
            OrionLib:Destroy()
          end    
    })    
end





-- Key System



_G.Key = "Parkerisbald"
_G.KeyInput = "string"

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function InCorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Inorrect Key!",
        Content = "You have entered the incorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
    Name = "Key",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})


Tab:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        _G.KeyInput = Value
    end
})

Tab:AddButton({
    Name = "Check Key",
    Callback = function()
        if _G.KeyInput == _G.Key then
            CorrectKeyNotification()
            MakeScriptHub()
        else
            InCorrectKeyNotification()
        end
    end
})
